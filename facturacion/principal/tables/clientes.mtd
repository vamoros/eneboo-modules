		<name>codedi</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Código edi (EAN)")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>17</length>
	</field>

	<field>
		<name>codsubcuenta</name>
		<!-- Código de la subcuenta de contabilidad asociada al cliente -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Subcuenta")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>
	</field>
	
	<field>
		<name>idsubcuenta</name>
		<!-- Identificador interno de la subcuenta -->
		<alias>QT_TRANSLATE_NOOP("MetaData","ID")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>co_subcuentas</table>
			<field>idsubcuenta</field>
			<card>M1</card>
			<checkIn>false</checkIn>
		</relation>
	</field>

	<field>
		<name>debaja</name>
		<!-- Indica si el cliente está de baja -->        
		<alias>QT_TRANSLATE_NOOP(&quot;MetaData&quot;,&quot;De baja&quot;)</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
		<default>false</default>
	</field>
	
	<field>
		<name>fechabaja</name>
		<!-- Fecha de baja -->        
		<alias>QT_TRANSLATE_NOOP(&quot;MetaData&quot;,&quot;Desde&quot;)</alias>
		<null>true</null>
		<pk>false</pk>
		<type>date</type>
		<default>null</default>
	</field>
	
	<field>
		<name>codcontacto</name>
		<!--Código del contacto principal-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Contacto")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
	</field>
	
	<field>
		<name>tipoidfiscal</name>
		<!--Tipo de identificación fiscal-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo Id. Fiscal")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>25</length>
		<optionslist>NIF,NIF/IVA,Pasaporte,Doc.Oficial País,Cert.Residencia,Otro</optionslist>
		<default>NIF</default>
	</field>

</TMD>
