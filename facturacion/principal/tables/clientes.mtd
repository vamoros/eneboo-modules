		<relation>
			<table>tiposrappel</table>
			<field>codtiporappel</field>
			<card>M1</card>
		</relation>

	</field>

	<field>
		<name>codagente</name>
		<!--Código del agente comercial asignado al cliente-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Agente comercial")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>

		<relation>
			<table>agentes</table>
			<field>codagente</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>telefono2</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Teléfono 2")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>30</length>
	</field>

