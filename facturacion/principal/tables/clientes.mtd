<!DOCTYPE TMD>
<TMD>
	<name>clientes</name>
	<!--Listado de datos de clientes-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Clientes")</alias>

	<field>
		<name>codcliente</name>
		<!--Código identificativo del cliente-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>string</type>
		<length>6</length>
		<counter>true</counter>

		<relation>
			<table>dirclientes</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>descuentosclientes</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>cuentasbcocli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>articuloscli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>contactosclientes</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>pedidoscli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>albaranescli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>

		<relation>
			<table>facturascli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>
		
		<relation>
			<table>co_subcuentascli</table>
			<field>codcliente</field>
			<card>1M</card>
		</relation>
		
		<relation>
			<table>i_facturascli</table> 
			<field>i_facturascli_codcliente</field>
			<card>1M</card>
		</relation>
		
		<relation>
			<table>co_operaciones349</table> 
			<field>codcliente</field>
			<card>1M</card>
		</relation>
		
		<relation>
			<table>tpv_datosgenerales</table> 
			<field>codcliente</field>
			<card>1M</card>
		</relation>
	
		<relation>
			<table>cl_proyectos</table> 
			<field>codcliente</field>
			<card>1M</card>
		</relation>
	
		<relation>
			<table>cl_incidencias</table> 
			<field>codcliente</field>
			<card>1M</card>
		</relation>
	
		<relation>
			<table>cl_comunicaciones</table> 
			<field>codcliente</field>
			<card>1M</card>
		</relation>
	</field>

	<field>
		<name>nombre</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
	</field>

	<field>
		<name>cifnif</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","C.I.F./N.I.F")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>20</length>
	</field>

	<field>
		<name>nombrecomercial</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre comercial")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
	</field>

	<field>
		<name>codgrupo</name>
		<!--Si el cliente pertenece a un grupo, el código del mismo-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Grupo clientes")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>

		<relation>
			<table>gruposclientes</table>
			<field>codgrupo</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>telefono1</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Teléfono 1")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>30</length>
	</field>

	<field>
		<name>contacto</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Contacto")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
		<editable>false</editable>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>observaciones</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Observaciones")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>

	<field>
		<name>codpago</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Forma de pago")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>

		<relation>
			<table>formaspago</table>
			<field>codpago</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>codcuentadom</name>
		<!--Código de cuenta corriente en la que se domicilian los recibos del cliente-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Domiciliar en")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
	</field>

	<field>
		<name>codcuentarem</name>
		<!--Código de cuenta corriente en la que se remesan los recibos del cliente-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Remesar en")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>

		<relation>
			<table>cuentasbanco</table>
			<field>codcuenta</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>coddivisa</name>
		<!--Si el cliente es extranjero, la divisa en que trabaja-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Divisa")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>3</length>

		<relation>
			<table>divisas</table>
			<field>coddivisa</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>codserie</name>
		<!--Serie de facturación asociada a cliente-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Serie")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>2</length>

		<relation>
			<table>series</table>
			<field>codserie</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>regimeniva</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Régimen I.V.A.")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>20</length>
		<optionslist>QT_TRANSLATE_NOOP("MetaData","General");QT_TRANSLATE_NOOP("MetaData","Exportaciones");QT_TRANSLATE_NOOP("MetaData","U.E.");QT_TRANSLATE_NOOP("MetaData","Exento")</optionslist>
		<default>QT_TRANSLATE_NOOP("MetaData","General")</default>
	</field>

	<field>
		<name>recargo</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Aplicar recargo de equivalencia")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
	</field>

	<field>
		<name>ivaincluido</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Facturar con I.V.A. incluido")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
	</field>

	<field>
		<name>riesgomax</name>
		<!--Cantidad máxima que el cliente podrá adeudar a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Riesgo máximo autorizado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
	</field>

	<field>
		<name>riesgoalcanzado</name>
		<!--Cantidad máxima que el cliente ha adeudado a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Riesgo alcanzado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
		<editable>false</editable>
	</field>

	<field>
		<name>capitalimpagado</name>
		<!--Cantidad impagada por el cliente a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Capital impagado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
	</field>

