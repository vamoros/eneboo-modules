<!DOCTYPE TMD>
<TMD>
	<name>reciboscli</name>
	<!-- Listado de recibos de clientes -->
	<alias>QT_TRANSLATE_NOOP("MetaData","Recibos de Clientes")</alias>

	<field>
		<name>idrecibo</name>
		<!-- Identificador interno -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>pagosdevolcli</table>
			<field>idrecibo</field>
			<card>1M</card>
		</relation>
	</field>

	<field>
		<name>codigo</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>
		<default>0</default>
		<editable>false</editable>
	</field>

	<field>
		<name>estado</name>
		<!-- Indica si el recibo está emitido, pagado o devuelto -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Estado")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>20</length>
		<calculated>true</calculated>
		<editable>false</editable>
	</field>

	<field>
		<name>importe</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Importe")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
	</field>
	
	<field>
		<name>fecha</name>
		<!-- Fecha de emisión -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Emisión")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>date</type>
		<editable>false</editable>
	</field>

	<field>
		<name>fechav</name>
		<!-- Fecha de vencimiento -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Vencimiento")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>date</type>
	</field>
	
	<field>
		<name>codcliente</name>
		<!-- Código del cliente -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Cod. cliente")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
		<editable>false</editable>

		<relation>
			<table>clientes</table>
			<field>codcliente</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>nombrecliente</name>
		<!-- Nombre del cliente -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
		<editable>false</editable>
	</field>

	<field>
		<name>idremesa</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Número de Remesa")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<default>0</default>
	</field>

