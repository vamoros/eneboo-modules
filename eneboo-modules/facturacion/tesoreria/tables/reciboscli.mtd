		<name>idfactura</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador Factura")</alias>
		<null>false</null>
		<ck>true</ck>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>facturascli</table>
			<field>idfactura</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>

	<field>
